{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDataset_sol",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {# &Item} as string,",
				"          creationtime as string,",
				"          herbicideTolerantFlag as string,",
				"          imageUrl_null as string,",
				"          modifiedtime as string,",
				"          owner as string,",
				"          productLine as string,",
				"          segmentCode as string,",
				"          insectprotection as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive({update on} = currentTimestamp()) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          herbicideTolerantFlag,",
				"          imageUrl_null,",
				"          modifiedtime,",
				"          owner,",
				"          productLine,",
				"          segmentCode,",
				"          insectprotection,",
				"          creationtime,",
				"          {update on}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}